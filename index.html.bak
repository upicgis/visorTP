        map.getPane('pane_Empreses_5').style['mix-blend-mode'] = 'normal';
        var layer_Empreses_5 = new L.geoJson(json_Empreses_5, {
            attribution: '',
            interactive: true,
            dataVar: 'json_Empreses_5',
            layerName: 'layer_Empreses_5',
            pane: 'pane_Empreses_5',
            onEachFeature: pop_Empreses_5,
            pointToLayer: function (feature, latlng) {
                var context = {
                    feature: feature,
                    variables: {}
                };
                return L.circleMarker(latlng, style_Empreses_5_0(feature));
            },
        });
        bounds_group.addLayer(layer_Empreses_5);
        map.addLayer(layer_Empreses_5);
        var baseMaps = {};
        L.control.layers(baseMaps,{'<img src="legend/Empreses_5.png" /> Empreses': layer_Empreses_5,'<img src="legend/Lmitsmunicipals_4.png" /> Límits municipals': layer_Lmitsmunicipals_4,'<img src="legend/PAE_3.png" /> PAE': layer_PAE_3,"Ortofoto [2021]": layer_Ortofoto2021_2,"Topogràfic 1:10.000": layer_Topogrfic110000_1,"Topogràfic 1:25.000": layer_Topogrfic125000_0,},{collapsed:false}).addTo(map);
        setBounds();
